{
  "name": "toggle-class",
  "displayName": "Toggle Class",
  "description": "Show / Hide the content of any \"class\" HTML attribute",
  "publisher": "damln",
  "homepage": "https://github.com/damln/vscode-toggle-class",
  "repository": {
    "type": "git",
    "url": "https://github.com/damln/vscode-toggle-class.git"
  },
  "license": "MIT",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "toggle-class.hideClasses",
        "title": "HTML: Hide Classes"
      },
      {
        "command": "toggle-class.showClasses",
        "title": "HTML: Show Classes"
      },
      {
        "command": "toggle-class.toggleClasses",
        "title": "HTML: Toggle Classes"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.84.0",
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.6"
  }
}
